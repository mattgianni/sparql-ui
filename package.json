{
  "name": "sparql-ui",
  "main": "./lib/sparql-ui",
  "version": "0.2.0",
  "description": "SPARQL support in Atom",
  "keywords": [
    "sparql",
    "query"
  ],
  "activationCommands": {
    "atom-workspace": [
      "sparql-ui:configure",
      "sparql-ui:runQuery",
      "sparql-ui:runUpdate",
      "sparql-ui:describeUri",
      "sparql-ui:inUri",
      "sparql-ui:outUri"
    ]
  },
  "repository": "https://github.com/mattgianni/sparql-ui",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.2.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  }
}
